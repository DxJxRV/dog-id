{
  "expo": {
    "name": "Mi Mascota Plus",
    "slug": "veterinary-control",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "mi-mascota-plus",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mimascotaplus.app",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mimascotaplus.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos of vaccine labels and pets."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your pets."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone to record veterinary consultations."
        }
      ],
      "expo-secure-store",
      "expo-asset",
      "expo-font",
      "@react-native-google-signin/google-signin",
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "c45bd479-8525-4d57-b267-e2b658c6c2c0"
      },
      "googleClientId": "474914302160-n9ei7n4kgjeq94mr6l067t4hnj0jrcr9.apps.googleusercontent.com"
    },
    "owner": "dxjx"
  }
}
